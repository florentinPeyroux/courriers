package courriers;

import java.util.*;

public class City {
	
	private String name;
	private ArrayList <Inhabitant> inhabitantListe;
	
	/* post box */
	
	public void Courriers(String name){
		this.name = name;
	}
	
	public String getName (){
		return this.name;
	}
	
	public void addInhabitant(Inhabitant inhabitant){
		inhabitantListe.add(inhabitant);
	}

}
