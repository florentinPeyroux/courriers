package courriers;

public class BankAccount {
	
	private int mount ;
	
	public void bankAccount(int mount){
		 this.mount = mount;
	 }
	
	publi int getBankAccount (){
		return this.mount;
	}
	
	public void setBankAccount(int mount){
		this.mount=mount;
	}
	 
	public void credit (int mount){
		this.mount=this.mount + mount;
	}
	
	public void dedit (int mount){
		this.mount=this.mount - mount;
	}
	

}
